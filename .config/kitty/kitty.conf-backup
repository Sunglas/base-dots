window_padding_width 15
placement_strategy center
sync_to_monitor no
repaint_delay 4
input_delay 2
background_opacity 0.9
enable_audio_bell no
clipboard_control write-clipboard write_primary read-clipboard-ask read-primary-ask
shell_integration enabled

map ctrl+shift+t scroll_line_down
map ctrl+shift+n scroll_line_up
map ctrl+shift+enter clear_terminal scroll active
map ctrl+shifft+r load_config_file
